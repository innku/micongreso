<% title "Message" %>

<p>
  <strong>Text:</strong>
  <%=h @message.text %>
</p>
<p>
  <strong>Member:</strong>
  <%=h @message.member_id %>
</p>

<p>
  <%= link_to "Edit", edit_message_path(@message) %> |
  <%= link_to "Destroy", @message, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", messages_path %>
</p>
